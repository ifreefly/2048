<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project 2048">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="H:/home/workspace/2048/jar/2048.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="Game"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="H:/home/workspace/2048/bin"/>
        </jar>
    </target>
</project>
